{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "ESNext"
    ],
    "baseUrl": "./src",                                  
    "paths": {
      "@/resources/*" : ["resources/*"],
      "@/utils/*" : ["utils/*"],
      "@/middleware/*" : ["middleware/*"],
    },   
    "resolveJsonModule": true,  
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
}